! *customization: -color
XTerm*termName: xterm-256color
XTerm*selectToClipboard: true

! special
XTerm*foreground:   #131313
XTerm*background:   #e4e4e4
XTerm*cursorColor:  #e4e4e4

! black
XTerm*color0:       #cccccc
XTerm*color8:       #e4e4e4

! red
XTerm*color1:       #af005f
XTerm*color9:       #af5f87

! green
XTerm*color2:       #1c5f5f
XTerm*color10:      #008787

! yellow
XTerm*color3:       #af871c
XTerm*color11:      #dfaf00

! blue
XTerm*color4:       #1c5f87
XTerm*color12:      #5f87af

! magenta
XTerm*color5:       #5f1c5f
XTerm*color13:      #875f87

! cyan
XTerm*color6:       #005f87
XTerm*color14:      #0087af

! white
XTerm*color7:       #afafaf
XTerm*color15:      #e4e4e4
